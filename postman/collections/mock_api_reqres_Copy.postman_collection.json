              "script": {
                "id": "5f8178d5-5779-41fc-8bdc-b35daf81be45",
                "exec": [
                  "console.log(\"pre-res for request\");\r",
                  "pm.test(\"status code is 200\", () => {\r",
                  "    // pm.response.to.have.status(200);\r",
                  "    pm.response.code == 200;\r",
                  "});\r",
                  "\r",
                  "pm.test(\"Content-Type is present\", () => {\r",
                  "    pm.response.to.have.header(\"Content-Type\");\r",
                  "})\r",
                  "\r",
                  "pm.test(\"Content-Type header is application/json\", () => {\r",
                  "    pm.expect(pm.response.headers.get('Content-Type')).to.eql(\"application/json\");\r",
                  "})\r",
                  "\r",
                  "pm.test(\"Response time to be below 200 ms\", () => {\r",
                  "    pm.expect(pm.response.responseTime).to.be.below(200);\r",
                  "})\r",
                  "\r",
                  "const jsonData = pm.response.json();\r",
                  "\r",
                  "pm.test(\"Test data type of the response\", () => {\r",
                  "    pm.expect(jsonData).to.be.an(\"array\");\r",
                  "    jsonData.forEach(user => {\r",
                  "        pm.expect(user.role).to.be.a(\"string\");\r",
                  "        pm.expect(user.salary).to.be.a(\"number\");\r",
                  "    })\r",
                  "\r",
                  "})\r",
                  "\r",
                  "pm.test(\"value of all fields of the user sacar\", () => {\r",
                  "    const sacarUser = jsonData.find(user => user.username === \"sacar\");\r",
                  "    console.log(sacarUser)\r",
                  "        pm.expect(sacarUser.id).to.eql(\"10\");\r",
                  "        pm.expect(sacarUser.role).to.eql(\"qa\");\r",
                  "        pm.expect(sacarUser.salary).to.eql(90000);\r",
                  "    \r",
                  "})\r",
                  "\r",
                  "var schema = {\r",
                  "  \"$schema\": \"http://json-schema.org/draft-04/schema#\",\r",
                  "  \"type\": \"array\",\r",
                  "  \"items\": {\r",
                  "    \"type\": \"object\",\r",
                  "    \"properties\": {\r",
                  "      \"username\": { \"type\": \"string\" },\r",
                  "      \"role\": { \"type\": \"string\" },\r",
                  "      \"salary\": { \"type\": \"number\" },\r",
                  "      \"id\": { \"type\": \"string\" }\r",
                  "    },\r",
                  "    \"required\": [\"username\", \"role\", \"salary\", \"id\"]\r",
                  "  }\r",
                  "}\r",
                  "\r",
                  "\r",
                  "pm.test('Schema is valid', () =>{\r",
                  "    pm.expect(tv4.validate(jsonData,schema)).to.be.true;\r",
                  "})"
                ],
